# GitHub API token for accessing repositories and issues
# Get one at: https://github.com/settings/tokens
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Anthropic API key for Claude models
# Get one at: https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# E2B API key for sandboxed code execution
# Get one at: https://e2b.dev/
E2B_API_KEY=e2b_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Agent configuration (optional - defaults shown)
E2B_TIMEOUT_MS=600000
AGENT_DEFAULT_MODEL=claude-sonnet-4.5-20250929
AGENT_MAX_THINKING_TOKENS=16000
AGENT_INTERACTIVE=true
